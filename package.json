{
  "name": "vite-music",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "tsc && eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint:fix": "tsc && eslint . --fix --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,tsx,ts,js,json,md}\"",
    "lint:css": "stylelint \"./**/*.{css,less,vue,html}\"",
    "lint:css:fix": "stylelint --fix \"./**/*.{css,less,vue,html}\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "element-plus": "^2.4.3",
    "pinia": "^2.1.7",
    "swiper": "^8.3.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-video-play": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.1.0",
    "style-import": "^0.0.8",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.4",
    "vue-tsc": "^1.8.5"
  }
}
